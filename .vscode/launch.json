{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/webroot/product/puretide/server.js",
            "cwd":     "${workspaceFolder}/webroot/product/puretide/"
        }, 
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Process",
            "processId": "${command:PickProcess}",
            "skipFiles": ["<node_internals>/**"]
          }        
    ]
}